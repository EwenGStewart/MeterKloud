{

  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/_framework/*", "/css/*", "/js/*", "/images/*", "/MeterView/*" ]
  },




  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github?post_login_redirect_uri=.referrer",
      "statusCode": 302
    }
  },


  "routes": [
    {
      "route": "/MeterView",
      "rewrite": "/MeterView/index.html",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/MeterView/index.htm",
      "rewrite": "/MeterView/index.html",
      "allowedRoles": [ "anonymous" ]
    },
    {
      "route": "/MeterView/*",
      "allowedRoles": [ "anonymous" ]
    }

  ]
}
