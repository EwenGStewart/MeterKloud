@page "/LoggedOut"
@inject NavigationManager Navigation 


<div class="container text-right">
    <AuthorizeView>
        <Authorized>
            <div>
                Hello, @context.User.Identity!.Name
                <a href="#" >Log out</a>
            </div>
        </Authorized>
        <NotAuthorized>
            <a href=".auth/login/github">Log in/Sign up</a>
        </NotAuthorized>
    </AuthorizeView>
</div>
